{
  "manifest_version": 3,
  "name": "TubeQuiet",
  "version": "1.0",
  "description": "Extens√£o do Chrome com React",
  "action": {
    "default_popup": "../src/popup/popup.html"
  },
  "permissions": ["activeTab", "scripting", "cookies", "tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
